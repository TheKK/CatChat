#: src/client_main.c:134
#, c-format
msgid "CatChat client (OS homework) 0.11\n"
msgstr "CatChat(貓聊) 用戶端 (作業系統作業) 0.11\n"

#: src/client_main.c:140
#, c-format
msgid "CatChat client usage:[-i ip address][-p port][-v version][-h help]\n"
msgstr "CatChat(貓聊) 用戶端 用法:[-i ip 位置][-p 埠號][-v 版本][-h 幫助]\n"

#: src/client_main.c:207
#, c-format
msgid "\r[SYSTEM] Initializing...\n"
msgstr "\r[系統] 初始化中...\n"

#: src/client_main.c:227
#, c-format
msgid "\r[SYSTEM] Quit...\n"
msgstr "\r[系統] 離開中...\n"

#: src/client_main.c:252
#, c-format
msgid "[SYSTEM] What is you name: "
msgstr "[系統] 貴姓大名: "

#: src/client_main.c:258
#, c-format
msgid "[SYSTEM] Space is not allowed, try again\n"
msgstr "[系統] 空白字元並不被允許，再試一次\n"

#: src/client_main.c:261
#, c-format
msgid "[SYSTEM] You type nothing, try again\n"
msgstr "[系統] 您什麼也沒輸入，再試一次\n"

#: src/client_main.c:272
#, c-format
msgid "[SYSTEM] This name has been uesed, use another\n"
msgstr "[系統] 這個名字已經被使用過，請換一個\n"

#: src/client_main.c:310
#, c-format
msgid "Help page work in progress\n"
msgstr "幫助頁面正在製作中\n"

#: src/client_main.c:313 src/client_main.c:323
#, c-format
msgid "[SYSTEM] Command <:%s> not found\n"
msgstr "[系統] 指令 <:%s> 不存在\n"

#: src/client_main.c:327
#, c-format
msgid "[SYSTEM] <:%s> is not a command\n"
msgstr "[系統] <:%s> 並不是一個指令\n"

#: src/client_main.c:438
#, c-format
msgid "\r[SYSTEM] File \"%s\" downloaded\n"
msgstr "\r[系統] 檔案 \"%s\" 下載完成\n"

#: src/client_main.c:455
#, c-format
msgid "\r[SYSTEM] Disconnected...\n"
msgstr "\r[系統] 連線中斷...\n"

#: src/client_main.c:459
#, c-format
msgid "\r[SYSTEM] To exit CatChat, please type :q<Enter>\n"
msgstr "\r[系統] 要離開 CatChat(貓聊)，請輸入 :q<Enter>\n"

#: src/client_main.c:462
#, c-format
msgid "\rTeaching you, is hard...(noise)\n"
msgstr "\r要教會你，很難...(雜音)\n"

#: src/client_main.c:463
#, c-format
msgid "\rSo you know what? You win, just go...(beep)\n"
msgstr "\r所以你知道嗎? 你贏了，快走吧...(嗶一聲)\n"

#: src/client_main.c:478
#, c-format
msgid "\r[SYSTEM] Connecting...\n"
msgstr "\r[系統] 連線中...\n"

#: src/client_main.c:482
#, c-format
msgid "\r[SYSTEM] Connected!!\n"
msgstr "\r[系統] 連線成功!!\n"

#: src/client_main.c:494
#, c-format
msgid "\r[SERVER] Chat room full, try again later\n"
msgstr "\r[伺服器] 聊天室客滿，請稍後再試\n"

#~ msgid "WIP\n"
#~ msgstr "努力製作中\n"
