#, c-format
msgid "WIP\n"
msgstr "努力製作中\n"

#: src/client_main.c:140
#, c-format
msgid "\r[SYSTEM] Initializing...\n"
msgstr "\r[系統] 初始化中...\n"

#: src/client_main.c:160
#, c-format
msgid "\r[SYSTEM] Quit...\n"
msgstr "\r[系統] 離開中...\n"

#: src/client_main.c:185
#, c-format
msgid "[SERVER] What is you name: "
msgstr "[伺服器] 貴姓大名: "

#: src/client_main.c:196
#, c-format
msgid "[SERVER] This name has been uesed, use another\n"
msgstr "[伺服器] 這個名字已經被使用過，請換一個\n"

#: src/client_main.c:220
#, c-format
msgid "Help page work in progress\n"
msgstr "幫助頁面正在製作中\n"

